{
  "version": 2,
  "builds": [
    {
      "src": "BE/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "FE/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "BE/app.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
